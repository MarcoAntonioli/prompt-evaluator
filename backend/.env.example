# OCI Configuration
# Copy this file to .env and fill in your actual values

# Required: Your OCI Compartment OCID
COMPARTMENT_OCID=ocid1.compartment.oc1..your_compartment_ocid

# Model IDs (at least one is required)
# You can comment out models you don't want to use by adding # at the start
OCI_COHERE_MODEL_ID=cohere.command
OCI_GEMINI_MODEL_ID=gemini.pro
OCI_GROK_MODEL_ID=grok-beta
OCI_LLAMA_MODEL_ID=meta.llama-2-70b-chat

# Optional: OCI Service Endpoint (defaults to us-chicago-1 if not set)
# Adjust the region as needed for your OCI deployment
OCI_SERVICE_ENDPOINT=https://inference.generativeai.us-chicago-1.oci.oraclecloud.com

# Optional: Database URL (defaults to SQLite if not set)
DATABASE_URL=sqlite:///./llm_comparison.db
